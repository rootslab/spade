    , "redis"
    , "client"
    , "redis client"
    , "Redis"
    , "LUA"
    , "cache"
    , "key/value"
    , "NoSQL"
  ]
  , "author": {
    "name": "Guglielmo Ferri"
    , "email": "44gatti@gmail.com"
  }
  , "dependencies": {
    "bolgia" : ">=2.8.2"
    , "cocker" : ">=0.16.0 && < 1.0.0"
